{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.7.1",
      "destination": "wwwroot/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "bootbox.js@5.5.2",
      "destination": "wwwroot/lib/bootbox.js/",
      "files": [
        "bootbox.min.js"
      ]
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate.css/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    },
    {
      "library": "exceljs@4.3.0",
      "destination": "wwwroot/lib/exceljs/",
      "files": [
        "exceljs.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "read-excel-file@5.2.25",
      "destination": "wwwroot/lib/read-excel-file/",
      "files": [
        "bundle/read-excel-file.min.js"
      ]
    },
    {
      "library": "Chart.js@3.6.2",
      "destination": "wwwroot/lib/Chart.js/",
      "files": [
        "chart.min.js"
      ]
    }
  ]
}